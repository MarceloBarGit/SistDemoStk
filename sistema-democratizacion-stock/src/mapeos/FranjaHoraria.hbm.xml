<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="datos.FranjaHoraria" table="franja_horaria">
		<id column="id_franja_horaria" name="idFranjaHoraria">
			<generator class="identity"/>
		</id>
		<property column="hora_inicio" name="horaInicio" type="int"/>
		<property column="hora_fin" name="horaFin" type="int"/>
		
	</class>
</hibernate-mapping>