<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="datos.Producto" table="producto">
		<id column="id_producto" name="idProducto">
			<generator class="identity"/>
		</id>
		<property column="codProducto" name="codProducto" type="string"/>
		<property column="nombre" name="nombre" type="string"/>
		<property column="descripcion" name="descripcion" type="string"/>
		<property column="habilitado" name="habilitado" type="boolean"/>
		<property column="precio_unitario" name="precioUnitario" type="float"/>
	</class>
</hibernate-mapping>